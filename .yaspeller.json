{
    "lang": "ru",
    "excludeFiles": 
    [
        ".git",
        "node_modules",
        ".vscode",
        "dist"
    ],
    "fileExtensions": 
    [
        ".md",
        ".html"
    ],
    "checkYo": true,
    "ignoreUppercase": true,
    "ignoreUrls": true,
    "dictionary": [
        "(б|Б)аг(и|ов|а|ом)?",
        "(?:в|В)уаля",
        "(?:с|С)таб(?:ы|а|ов)?",
        "(?:м|М)ок(?:и|а|ов|ах|ать)?",
        "(?:ф|Ф)реймворк(?:и|а|е|ов|ом)?",
        "(?:б|Б)иткоин(?:ы|а|ов|у)?",
        "(?:ф|Ф)ич(?:и|а)?",
        "билд(?:ов)?",
        "раннер(?:ами)?",
        "кодогенераци(?:ю)",
        "невиртуальн(?:ые)",
        "(?:р|Р)ефакторинг(?:а|у)?",
        "сетап(?:ов)?",
        "солюшен(?:е)?",
        "продакшн",
        "инстанцируются",
        "инстанцировани(?:я)",
        "контрол(?:ы|а|ов)?",
        "месседжбокс(?:ы)?",
        "Д'Аддамио",
